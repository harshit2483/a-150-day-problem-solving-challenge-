class Solution:
    def removeElement(self, nums: List[int], val: int) -> int:
        n=len(nums)
        i=0
        c=0
        while(i<n):
            if(nums[i]==val):
                c+=1
            i+=1
        i=0
        while(i<c):
            nums.remove(val)
            i+=1
            
        
